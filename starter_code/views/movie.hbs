<div>
  <a href="/movies">Go back</a>
  
  <img src="{{movies.image}}" alt="">
  <p>{{movies.title}}</p>
  <p>{{movies.director}}</p>
  <p>Stars</p>
  <ul>
    {{#each movies.stars}}
    <li>{{this}}</li>
    {{/each}}
  </ul>
  <p>{{movies.description}}</p>
  <ul>
    {{#each movies.showtimes}}
    <li>{{this}}</li>
    {{/each}}
  </ul>
</div>

